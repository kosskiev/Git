# возврат на предыдющую рерсию комита

# проверка логов
git log
# проверка последнего лога 
git log -1
# проверка последних n(в данном случае 3-х) логов для конкретного файла
git log -3 namefile

# для возвращения на определенный комин нужен хэш комита (пример 2ca0a03852907c979d429febb599e0c86ae0cd20)
git chekout 2ca0a03852907c979d429febb599e0c86ae0cd20

